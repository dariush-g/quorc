@import::<string.qu>;
@import::("lexer/lex.qu");


def main(argc: int, argv: char**) :: int {
    
    if (argc != 2) {
        exit(1);
    }
    
    argv = argv + 1;

    let filename: char* = *argv;


    
    let name_len: int = strlen(filename);

    if (char_at(filename, name_len - 1) != 'u' && 
        char_at(filename, name_len - 2) != 'q' && 
        char_at(filename, name_len - 3) != '.') {
        exit(1);
    }

    return 0;
}
